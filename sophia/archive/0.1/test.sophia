integer fib (integer n):

	if n = 0:
		return 0
	a: 0
	b: 1
	i: 1
	while i < n:
		c: b
		b: a + b
		a: c
		i: i + 1
	else:
		return b

integer increment (integer n):

	print(n)
	return increment(n + 1)

integer b (integer n):

	if n = 0:
		return n
	else if n = 1:
		return n
	else:
		return b(n - 1) + b(n - 2)


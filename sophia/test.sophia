type even extends int:

	constraint:
		even % 2 = 0

int fib (even):

	int a: 0
	int b: 1
	int i: 0
	while i < even:
		c: b
		b: a + b
		a: c
		i: i + 1
	return a

int increment (int n):

	int i: n
	while true:
		i: i + 1
		if i > 10:
			yield null
		else:
			yield i

int x <- increment(0)
print(x)
while true:
	assert null -> x:
		print(x)
	else:
		print('!')
		break